{
    "name": "chiswill/ep-helper",
    "description": "Base Helper",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpbench/phpbench": "^0.17.1",
        "phpunit/phpunit": "^9.4",
        "vimeo/psalm": "^4.5"
    },
    "suggest": {
        "ext-SimpleXML": "Allows to use Arr::fromXml()"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Ep\\Helper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ep\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}